go2see.xyz:8443
{
    proxy / https://jack6liu.wordpress.com/ {
        header_upstream -Origin
    }
}

ws0.go2see.xyz:8443
{
    proxy /ray 127.0.0.1:12631 {
        websocket
        header_upstream -Origin
        insecure_skip_verify
    }
    timeouts none
}

h20.go2see.xyz:8443
{
    proxy /h2 https://127.0.0.1:12611 {
        insecure_skip_verify
        header_upstream X-Forwarded-Proto "https"
        header_upstream Host "h20.go2see.xyz"    
    }
}

